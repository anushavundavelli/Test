{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aotestadf"
		},
		"AzureDataLakeStorageGen2LinkedService_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageGen2LinkedService'"
		},
		"AzureDataLakeStorageGen2LinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://aotestadlsgen2.dfs.core.windows.net"
		},
		"ODataLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://services.odata.org/AdventureWorksV3/AdventureWorks.svc"
		},
		"OATesttrigger_properties_Copy Data_parameters_year": {
			"type": "string",
			"defaultValue": "@trigger().startTime"
		},
		"TestTrigger_properties_Copy Data_parameters_year": {
			"type": "string",
			"defaultValue": "formatDateTime(@trigger().startTime,yyyy)"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "aotestadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageGen2LinkedService_accountKey": {
						"value": "[parameters('AzureDataLakeStorageGen2LinkedService_accountKey')]"
					},
					"AzureDataLakeStorageGen2LinkedService_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageGen2LinkedService_properties_typeProperties_url')]"
					},
					"ODataLinkedService_properties_typeProperties_url": {
						"value": "[parameters('ODataLinkedService_properties_typeProperties_url')]"
					},
					"OATesttrigger_properties_Copy Data_parameters_year": {
						"value": "[parameters('OATesttrigger_properties_Copy Data_parameters_year')]"
					},
					"TestTrigger_properties_Copy Data_parameters_year": {
						"value": "[parameters('TestTrigger_properties_Copy Data_parameters_year')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}