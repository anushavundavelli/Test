{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "aotestadf"
		},
		"ODataLinkedService_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://services.odata.org/AdventureWorksV3/AdventureWorks.svc"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ODataLinkedService')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"description": "Reads data for test purposes",
				"annotations": [],
				"type": "OData",
				"typeProperties": {
					"url": "[parameters('ODataLinkedService_properties_typeProperties_url')]",
					"authenticationType": "Anonymous"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/ODataCompanySales_Dataset')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "ODataLinkedService",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "ODataResource",
				"schema": [],
				"typeProperties": {
					"path": "CompanySales"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/ODataLinkedService')]"
			]
		}
	]
}